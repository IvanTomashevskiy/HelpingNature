<section class="main ">
	<ul class="profile ">
		<% if (userLogged != null) { %>
			<li class="profile__user">
				<img src="/images/avatars/<%= userLogged.avatar %>" class="profile__img">
				<a class="profile__dropdown-toggle " href="#" id="dropUser" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"> <%= userLogged.full_name %>
				</a>
				<div class="profile__menu  " aria-labelledby="dropUser">
					<a class="profile__button profile__button_profile " href="/users/profile">My profile</a>
					<div class="dropdown-divider"></div>
					<a class="profile__button profile__button_logout " href="/users/logout">Logout</a>
				</div>
			</li>
		<% } else { %>
			<a class="main__button main__button_reg" href="/users/register">Register</a>
			<a class="main__button main__button_login" href="/users/login">Login</a>
		<% } %>
	</ul>
	<a class="main__title" href="/">Helping Nature</a>
